<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <!-- Export method for Developer ID distribution -->
    <key>method</key>
    <string>developer-id</string>
    
    <!-- Team ID for code signing -->
    <key>teamID</key>
    <string>$(DEVELOPMENT_TEAM)</string>
    
    <!-- Upload symbols for crash reporting -->
    <key>uploadSymbols</key>
    <true/>
    
    <!-- Upload bitcode (not applicable for macOS) -->
    <key>uploadBitcode</key>
    <false/>
    
    <!-- Compile bitcode (not applicable for macOS) -->
    <key>compileBitcode</key>
    <false/>
    
    <!-- Strip Swift symbols -->
    <key>stripSwiftSymbols</key>
    <true/>
    
    <!-- Thinning for specific devices (not applicable for macOS) -->
    <key>thinning</key>
    <string>&lt;none&gt;</string>
    
    <!-- Destination for export -->
    <key>destination</key>
    <string>export</string>
    
    <!-- Signing style -->
    <key>signingStyle</key>
    <string>automatic</string>
    
    <!-- Provisioning profiles (automatic for Developer ID) -->
    <key>provisioningProfiles</key>
    <dict/>
    
    <!-- Signing certificate -->
    <key>signingCertificate</key>
    <string>Developer ID Application</string>
    
    <!-- App Store distribution options (not used for Developer ID) -->
    <key>generateAppStoreInformation</key>
    <false/>
    
    <!-- Installer signing (for PKG distribution) -->
    <key>installerSigningCertificate</key>
    <string>Developer ID Installer</string>
</dict>
</plist>


